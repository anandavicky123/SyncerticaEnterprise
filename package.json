{
  "name": "syncerticaenterprise",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:test": "dotenv -e .env.test -- prisma db seed",
    "db:migrate:test": "dotenv -e .env.test -- prisma migrate deploy",
    "db:reset": "prisma migrate reset --force",
    "db:reset:test": "dotenv -e .env.test -- prisma migrate reset --force",
    "docker:build": "docker build -t syncertica-enterprise .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker system prune -f",
    "infra:validate": "echo 'Infrastructure validation removed'",
    "security:audit": "npm audit --audit-level high",
    "workflow:setup": "node scripts/setup-workflow.js",
    "workflow:test": "gh workflow run ci-cd.yml",
    "setup:secrets": "node scripts/setup-github-secrets.js",
    "setup:dynamodb": "node scripts/setup-dynamodb.js",
    "security:fix": "npm audit fix",
    "deploy:staging": "npm run build",
    "deploy:production": "npm run build",
    "ci": "npm run lint && npm run type-check && npm run test && npm run build",
    "prepare": "husky install || true",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.883.0",
    "@aws-sdk/lib-dynamodb": "^3.883.0",
    "@azure/cosmos": "^4.5.0",
    "@azure/functions": "^4.7.2-preview",
    "@azure/identity": "^4.11.1",
    "@modelcontextprotocol/sdk": "^1.17.3",
    "@prisma/client": "^6.16.2",
    "@types/pg": "^8.15.4",
    "@types/uuid": "^10.0.0",
    "argon2": "^0.44.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "openid-client": "^5.7.0",
    "pg": "^8.16.3",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.1.0",
    "redis": "^5.6.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "uuid": "^12.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "prisma": "^6.16.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
